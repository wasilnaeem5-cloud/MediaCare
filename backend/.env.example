# Server Configuration
PORT=5000
NODE_ENV=production

# MongoDB Atlas Connection String
# Replace <username>, <password>, and <cluster-url> with your Atlas credentials
MONGO_URI=mongodb+srv://<username>:<password>@cluster0.xxxx.mongodb.net/medicareDB?retryWrites=true&w=majority

# Security
JWT_SECRET=your_super_secret_startup_key_here

# Notes:
# 1. Ensure your IP is whitelisted in MongoDB Atlas Network Access.
# 2. If using a special character in your password, URL-encode it.
# 3. The medicareDB at the end of the URI is the database name.
